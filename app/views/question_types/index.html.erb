<h1>
    Listing Question Types
</h1>

<table class="table table-striped">
    <tr>
        <th>Question Type </th>
        <th>Contest Id </th>
        <th>Actions </th>
    </tr>
<% @question_types.each do |q| %>
    <tr>
        <td> <%= link_to q.question_type, q %></td>
        <td> <%= q.contest_id %></td>
        <td> <%= link_to 'Edit', edit_question_type_path(q)%>&nbsp;&nbsp;&nbsp;
            <%= link_to 'Delete', q, :confirm => 'Are you sure?', :method => :delete %> </td>
    </tr>     
<% end %>
</table>
<br/>
<%= link_to 'New Question Type', new_question_type_path %>

